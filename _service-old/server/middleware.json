{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../client/favicon.ico"
    }
  },
  "initial": {
    "loopback-context#per-request": {},
    "compression": {},
    "helmet": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": false,
        "log": true
      }
    }
  }
}