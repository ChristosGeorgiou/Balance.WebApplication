{
  "$id": "https://balnc.cgeosoft.com/schemas/board",
  "$schema": "http://json-schema.org/schema#",
  "title": "Board",
  "type": "object",
  "version": 0,
  "disableKeyCompression": true,
  "additionalProperties": false,
  "properties": {
    "key": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "members": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "created": {
      "type": "number"
    },
    "avatar": {
      "type": "string"
    }
  },
  "required": [
    "key",
    "name"
  ]
}